<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.example.gifthavenbackend.entity.InventoryEntity" table="inventory" schema="gift_shop">
        <id name="inventoryId" column="inventory_id"/>
        <property name="giftId" column="gift_id"/>
        <property name="number" column="number"/>
        <property name="purchasePrice" column="purchase_price"/>
        <property name="createAt" column="create_at"/>
        <property name="updateAt" column="update_at"/>
        <property name="deleted" column="deleted"/>
    </class>
</hibernate-mapping>